<h1 class="title">Productos</h1>
<div class="container">

  {{#each products}}
    <div class="product">
      <h3>{{this.title}}</h3>
      <h4>{{this.author}}</h4>
      <h5>{{this.category}}</h5>
      <p>Description: {{this.description}}</p>
      <p>ID: {{this.id}}</p>
      <p>$ {{this.price}}</p>
      <p>Stock: {{this.stock}}</p>
    </div>

  {{/each}}

</div>